FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 25.0516;//5.51135;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 25.0516;//5.51135;
    }
    outlet
    {
        type            zeroGradient;
    }
    axi_symm-f
    {
        type            wedge;
    }
    axi_symm-r
    {
        type            wedge;
    }
    wall
    {
        type              fixedValue;
        value             uniform 404972.8778; //according to NASA turbmodel webpage
    }
    defaultFaces
    {
        type            empty;
    }
}


